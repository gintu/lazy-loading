{
  "name": "lazy-load",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://gintu.github.io/lazy-loading",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.47",
    "@types/react": "^16.14.24",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.23",
    "postcss-cli": "^9.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "typescript": "^4.1.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run run:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "run:css": "tailwindcss -i src/assets/css/tailwind.css -o src/assets/css/main.css",
    "watch:css": "tailwindcss -i src/assets/css/tailwind.css -o src/assets/css/main.css --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "gh-pages": "^3.2.3",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23"
  }
}
